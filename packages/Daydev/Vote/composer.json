{
    "name": "daydev/vote",
    "description": "Allows users to upvote and downvote user created content. This package is for tutorial purposes only.",
    "license": "MIT",
    "authors": [
        {
            "name": "adam",
            "email": "adamday@daydevelops.com",
            "homepage": "daydevelops.com"
        }
    ],
    "homepage": "https://github.com/daydev/vote",
    "keywords": ["Laravel", "Vote"],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Daydev\\Vote\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Daydev\\Vote\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Daydev\\Vote\\VoteServiceProvider"
            ],
            "aliases": {
                "Vote": "Daydev\\Vote\\Facades\\Vote"
            }
        }
    }
}
